{
  "version": "0.1.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "react",
    "state",
    "hooks",
    "react-elf",
    "react state"
  ],
  "description": "Manage your project states like redux with react hooks",
  "files": [
    "/dist",
    "/README.MD",
    "/LICENSE"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-elf",
  "author": "xiangli",
  "module": "dist/react-elf.esm.js",
  "size-limit": [
    {
      "path": "dist/react-elf.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-elf.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "husky": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "scheduler": "^0.20.2",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.172",
    "lodash": "^4.17.21"
  }
}
